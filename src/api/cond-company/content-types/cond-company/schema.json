{
  "kind": "collectionType",
  "collectionName": "cond_companies",
  "info": {
    "singularName": "cond-company",
    "pluralName": "cond-companies",
    "displayName": "Empresa",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "condomino": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::cond-owner.cond-owner",
      "mappedBy": "empresa"
    },
    "opening_hours": {
      "type": "string"
    },
    "cellphone": {
      "type": "customField",
      "customField": "plugin::strapi-phone-validator.phone"
    },
    "email": {
      "type": "string"
    },
    "description": {
      "type": "richtext",
      "maxLength": 1200
    },
    "whatsapp": {
      "type": "boolean"
    },
    "telephone": {
      "type": "customField",
      "options": {
        "country": "br"
      },
      "customField": "plugin::strapi-phone-validator.phone"
    },
    "logo": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "thumbnail": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "salas": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::room.room",
      "inversedBy": "empresas"
    },
    "torres": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::cond-unit.cond-unit",
      "inversedBy": "empresas"
    },
    "categorias": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "empresas"
    },
    "social_media": {
      "type": "boolean",
      "default": false
    },
    "facebook": {
      "type": "string"
    },
    "instagram": {
      "type": "string"
    }
  }
}
